SV=/home/ernst/projects/sv-benchmarks/c/aws-c-common

DEFINES += -DCBMC_NO_FORALL -DCBMC_NO_ASSERT_H

CS=$(SV)/prelude.h $(DEPENDENCIES) $(ENTRY).c
IS=$(CS:%.c=%.i)

I=$(SV)/$(ENTRY).i
C=$(SV)/$(ENTRY).c

sv-benchmark: $(I)
	@echo $(ENTRY)

$(C): $(CS)
	@cat $^ > $@

%.i: %.c
	@cpp $(INC) $(DEFINES) -o $@ $<
	@sed '/^#/ d' $@ -i # get rid of debug statements with lines
